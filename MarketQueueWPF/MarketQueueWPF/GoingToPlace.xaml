<Window x:Class="MarketQueueWPF.GoingToPlace"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MarketQueueWPF"
        mc:Ignorable="d"
        Title="GoingToPlace" Height="150" Width="300">
    <Grid Margin="10">
        <TextBlock Text="In how many minutes will you leave?" VerticalAlignment="Top" HorizontalAlignment="Center"
                   FontSize="16" FontWeight="DemiBold" FontFamily="Times New Roman"/>
        <TextBox Width="100" Height="20" x:Name="VisitTime" />



    </Grid>
</Window>
